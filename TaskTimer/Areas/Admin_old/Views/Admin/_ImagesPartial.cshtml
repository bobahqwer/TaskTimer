@model System.Data.Entity.DbSet<TaskTimer.Models.Images>
<div id="loadingLayer"><div class="loadingText">Loading ... </div></div>
<table id="myTable" class="tablesorter">
    <thead>

    <tr>
        <th>
            ID
        </th>
        <th>
            Name
        </th>
        <th>
            FAQ Title
        </th>
        <th>
            FAQ Question
        </th>
        <th>
            FAQ Answer
        </th>
        <th>
            Actions
        </th>
    </tr>
    </thead>

    <tbody>
    @foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FAQ.Title)
        </td>
        <td>
            @Html.Raw(item.FAQ.Question)
        </td>
        <td>
            @Html.Raw(item.FAQ.Answer)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
   } 
        </tbody>
</table>
